<div class="">
    <h3 class="text-white bg-success p-3 ">List of Bookings</h3>
     <table style="" class="table table-striped table-hover table-bordered ">
        <thead>
            <tr>
                <th>#</th>
                <th>Name</th>
                <th>Description</th>
                <th>Time</th>
            
                <th>Status</th>
            </tr>
        </thead>
        <tbody>
            {{#each users}}
            <tr>
                <td>{{_id}}</td>
                <td>{{username}}</td>
                <td>{{futsal.name}}</td>
                <td>{{futsal.time}}</td>
                <td>{{{adminStatus futsal.status}}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table> 
</div>
